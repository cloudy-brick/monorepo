{
  "root": "apps/node/microservices/core/node-lambda-microservice",
  "sourceRoot": "apps/node/microservices/core/node-lambda-microservice/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/node/microservices/core/node-lambda-microservice",
        "main": "apps/node/microservices/core/node-lambda-microservice/src/main.ts",
        "tsConfig": "apps/node/microservices/core/node-lambda-microservice/tsconfig.app.json",
        "assets": [
          "apps/node/microservices/core/node-lambda-microservice/src/app/lambdas/file.pdf"
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/node/microservices/core/node-lambda-microservice/src/environments/environment.ts",
              "with": "apps/node/microservices/core/node-lambda-microservice/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "node-microservices-core-node-lambda-microservice:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/node/microservices/core/node-lambda-microservice/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/node/microservices/core/node-lambda-microservice"
      ],
      "options": {
        "jestConfig": "apps/node/microservices/core/node-lambda-microservice/jest.config.js",
        "passWithNoTests": true
      }
    },
    "docker": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "push": false,
        "tags": [
          "cloudy-brick/node-lambda-microservice:latest",
          "cloudy-brick/node-lambda-microservice:v1"
        ]
      },
      "configurations": {
        "publish": {
          "push": true
        }
      }
    }
  },
  "tags": ["node", "microservice", "lambda"]
}
